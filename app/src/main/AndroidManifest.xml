<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.semmi.pickme">

    <application
        android:name=".dagger.PickMeApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".register.RegisterActivity"
            android:label="@string/title_activity_register"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".team.addteam.AddTeamActivity"
            android:label="@string/title_activity_add_team"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".team.list.TeamListActivity"
            android:label="@string/title_activity_team_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".vote.add_vote.AddVoteActivity"
            android:label="@string/title_activity_add_vote"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>